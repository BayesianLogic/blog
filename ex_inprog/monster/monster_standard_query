query value(t);
query !MonsterAtDoor(0, t);
query reward(Prev(t));
query exists Monster m DoorOf(m, t) == -1;
query exists Sound s (Time_Sound(s)==t) & ApparentDoor(s, t) == -1;
query exists Integer d  d < MaxDoor & SoundAroundDoor(d, t) & false;
query exists Integer d  d < MaxDoor & MonsterAtDoor(d, t) & false;
query exists Integer d  d < MaxDoor & MonsterAroundDoor(d, t) & false;
