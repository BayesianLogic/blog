// Type Declarations
type Aircraft;
type Blip;

// Origin Declarations
origin Aircraft Source(Blip);
origin Timestep Time(Blip);

// Number Declarations
#Aircraft ~ Poisson(5);
#Blip(Source=a, Time=t) ~ Bernoulli(0.8);
#Blip(Time=t) ~ Poisson(10);

// Evidence
obs #{Blip b:Time=@0}= 3;
obs #{Blip b:Time=@1}= 4;
obs #{Blip b:Time=@3}= 5;

// Query
query #{Aircraft a};
