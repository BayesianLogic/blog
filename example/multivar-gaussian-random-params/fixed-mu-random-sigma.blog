fixed RealMatrix mu0 = [0.0; 1.0];
fixed RealMatrix sigma0 = [1.0, 0.0; 0.0, 1.0];

random RealMatrix row1 ~ MultivarGaussian(mu0, sigma0);
random RealMatrix row2 ~ MultivarGaussian(mu0, sigma0);

fixed RealMatrix mu = [0.0; 1.0];
random RealMatrix sigma = vstack(transpose(row1), transpose(row2));

random RealMatrix x ~ MultivarGaussian(mu, sigma);

query x;
